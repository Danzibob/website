MOVW 0xFF
MOVWR TRISB
MOVW 0x00
MOVWR TRISA

MAIN:
    MOVRW PORTB
    ANDW 0x80
    JPZ MAIN
    INC 0x90
    CALL DIV
    MOVRW 0x9E
    XORW 0x00
    JPZ ZERO
    MOVRW 0x9E
    XORW 0x01
    JPZ ONE
    MOVRW 0x9E
    XORW 0x02
    JPZ TWO
    MOVRW 0x9E
    XORW 0x03
    JPZ THREE
    MOVRW 0x9E
    XORW 0x04
    JPZ FOUR
    MOVRW 0x9E
    XORW 0x05
    JPZ FIVE
    MOVRW 0x9E
    XORW 0x06
    JPZ SIX
    MOVRW 0x9E
    XORW 0x07
    JPZ SEVEN
    MOVRW 0x9E
    XORW 0x08
    JPZ EIGHT
    MOVRW 0x9E
    XORW 0x09
    JPZ NINE
TENS:
    MOVWR PORTA
    MOVRW 0x9E
    XORW 0x00
    JPZ UNIT
    MOVRW 0x9E
    XORW 0x01
    JPZ TEN
    MOVRW 0x9E
    XORW 0x02
    JPZ TWENTY
    MOVRW 0x9E
    XORW 0x03
    JPZ THIRTY
    MOVRW 0x9E
    XORW 0x04
    JPZ FOURTY
    MOVRW 0x9E
    XORW 0x05
    JPZ FIFTY
    MOVRW 0x9E
    XORW 0x06
    JPZ SIXTY
    MOVRW 0x9E
    XORW 0x07
    JPZ SEVENTY
    MOVRW 0x9E
    XORW 0x08
    JPZ EIGHTY
    MOVRW 0x9E
    XORW 0x09
    JPZ NINETY
    MOVW PORTA
    JMP MAIN
ZERO:
	MOVW 0x7D
	JMP TENS
ONE:
	MOVW 0x05
	JMP TENS
TWO:
	MOVW 0x5B
	JMP TENS
THREE:
	MOVW 0x4F
	JMP TENS
FOUR:
	MOVW 0x27
	JMP TENS
FIVE:
	MOVW 0x6E
	JMP TENS
SIX:
	MOVW 0x7E
	JMP TENS
SEVEN:
	MOVW 0x45
	JMP TENS
EIGHT:
	MOVW 0x7F
	JMP TENS
NINE:
	MOVW 0x6F
	JMP TENS

UNIT:
	MOVW 0xFD
	RET
TEN:
	MOVW 0x85
	RET
TWENTY:
	MOVW 0xDB
	RET
THIRTY:
	MOVW 0xCF
	RET
FOURTY:
	MOVW 0xA7
	RET
FIFTY:
	MOVW 0xEE
	RET
SIXTY:
	MOVW 0xFE
	RET
SEVENTY:
	MOVW 0xC5
	RET
EIGHTY:
	MOVW 0xFF
	RET
NINETY:
	MOVW 0xEF
	RET



DIV:
    MOVWR 0x9E
    MOVW 0x00
    MOVWR 0x9F
DIVMAIN:
    MOVRW 0x9E
	SUBW 0x0A
    INC 0x9F
    MOVWR 0x9E
    ANDW 0x80
    JPZ DIVMAIN
FIX:
    MOVRW 0x9E
    ADDW 0x0A
    MOVWR 0x9E
    DEC 0x9F
    RET
